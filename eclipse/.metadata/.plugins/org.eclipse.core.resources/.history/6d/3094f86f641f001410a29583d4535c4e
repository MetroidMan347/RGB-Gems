package mm347.rgbgems2.item;

import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;
import cpw.mods.fml.common.registry.GameRegistry;

public class ModItems {
	public static final Item gemRubite = new ItemBasic("gemRubite");
	public static final Item gemSapphirite = new ItemBasic("gemSapphirite");
	public static final Item gemEmerite = new ItemBasic("gemEmerite");
	
	public static void init() {
		GameRegistry.registerItem(gemRubite, "gemRubite");
		GameRegistry.registerItem(gemSapphirite, "gemSapphirite");
		GameRegistry.registerItem(gemEmerite, "gemEmerite");
	}
}